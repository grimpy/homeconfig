Xft.dpi: 96
Xft.autohint: 0
Xft.lcdfilter:  lcddefault
Xft.hintstyle:  hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb

! special
*.foreground:   #c0c5ce
*.background:   #2b303b
*.cursorColor:  #c0c5ce

! black
*.color0:       #2b303b
*.color8:       #65737e

! red
*.color1:       #bf616a
*.color9:       #bf616a

! green
*.color2:       #a3be8c
*.color10:      #a3be8c

! yellow
*.color3:       #ebcb8b
*.color11:      #ebcb8b

! blue
*.color4:       #8fa1b3
*.color12:      #8fa1b3

! magenta
*.color5:       #b48ead
*.color13:      #b48ead

! cyan
*.color6:       #96b5b4
*.color14:      #96b5b4

! white
*.color7:       #c0c5ce
*.color15:      #eff1f5



*matcher.button: 3
*colorUL: #86A2BB
URxvt.url-launcher: /usr/bin/firefox
URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: false
URxvt.font: xft:DejaVu Sans Mono for Powerline:size=9
URxvt.perl-ext-common: default,clipboard,keyboard-select,confirm-paste,url-select,font-size,vtwheel
URxvt.matcher.pattern.1: \\w{8}-\\w{4}-\\w{4}-\\w{4}-\\w{12}
URxvt.matcher.pattern.2: (https?|ftps?)://[^\\s]+
URxvt.keysym.C-T: perl:keyboard-select:activate
URxvt.keysym.C-U: perl:url-select:select_next
URxvt.keysym.C-C: perl:clipboard:copy
URxvt.keysym.C-V: perl:clipboard:paste
URxvt.iso14755 : false
URxvt.clipboard.copycmd:  xsel -ib
URxvt.clipboard.pastecmd: xsel -ob
URxvt.keysym.C-minus: perl:font-size:decrease
URxvt.keysym.C-equal: perl:font-size:increase
URxvt.shading:    20
URxvt.transparent:false
URxvt.cutchars:'"'&()*,;<=>?@[]^{|}
URxvt.termName: rxvt-unicode-256color
URxvt.urgentOnBell: True
URxvt.saveLines: 10000
! do not scroll with output
URxvt*scrollTtyOutput: false
! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true
! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

XTerm*faceName: xft:DejaVu Sans Mono for Powerline:size=9
